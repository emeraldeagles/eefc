{
  "name": "eefc",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "cypress:open": "./node_modules/.bin/cypress open",
    "cypress:run": "./node_modules/.bin/cypress run",
    "cypress:run:flow": "./node_modules/.bin/cypress run --spec cypress/integration/e2e/$FLOW/_index.spec.ts",
    "dev": "remix dev",
    "dev:hmr": "remix dev -c \"node ./server.js\"",
    "format:check": "pnpm exec prettier --check app --cache --cache-strategy metadata",
    "format:write": "pnpm exec prettier --write app --cache --cache-strategy metadata",
    "hook": "pnpm lint && pnpm format:check",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky install",
    "start": "remix-serve build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/api",
    "/coverage",
    ".eslintrc.js",
    "tailwind.config.js",
    "server.js",
    "remix.config.js"
  ],
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@vercel/remix": "2.5.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "date-fns": "^3.5.0",
    "embla-carousel-react": "^8.0.0",
    "isbot": "^3.6.1",
    "lucide-react": "^0.358.0",
    "react": "^18.2.0",
    "react-day-picker": "^8.10.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.8.1",
    "@remix-run/eslint-config": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.0.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^15.0.6",
    "@types/node": "20.5.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.2.0",
    "@vitest/coverage-c8": "^0.33.0",
    "c8": "^7.11.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^8.10.0",
    "happy-dom": "^9.18.3",
    "husky": "^8.0.0",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.0",
    "vite": "^5.0.0",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.2"
  },
  "engines": {
    "node": ">=14"
  }
}